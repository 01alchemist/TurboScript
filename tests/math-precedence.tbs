export function testF32_1(a: float32, b: float32, c: float32, d: float32): float32 {
    return (a + b) * (c + d);
}
export function testF32_12(a: float32, b: float32, c: float32, d: float32): float32 {
    return a + b * c + d;
}

export function testF32_2(a: float32, b: float32, c: float32, d: float32): float32 {
    return (a + b) * (c + (a * d));
}

export function test_1(a: float64, b: float64): float64 {
    return a + b;
}
export function test_2(a: float64, b: float64): float64 {
    return a - b;
}
export function test_3(a: float64, b: float64): float64 {
    return test_1(a, test_2(2.0, b));
}

