import {Math} from "javascript";

export function sin (value:float64):float64 {
    return Math.sin(value);
}

export function cos (value:float64):float64 {
    return Math.cos(value);
}

export function tan (value:float64):float64 {
    return Math.tan(value);
}

export function abs_f32 (value:float32):float32 {
    return Math.abs(value);
}
