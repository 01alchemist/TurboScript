// null number will convert to zero
let null_value:int32 = null;
let zero_value:int32 = 0;
export function compareNull():boolean {
    return null_value == null;
}
export function compareZero():boolean {
    return null_value == zero_value;
}
export function compareLocalNull():boolean {
    let null_value:int32 = null;
    return null_value == null;
}
export function compareLocalZero():boolean {
    let null_value:int32 = null;
    let zero_value:int32 = 0;
    return null_value == zero_value;
}
