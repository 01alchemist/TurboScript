export function testi32(): int32 {
    let a = 1;
    a--;
    return a;
}

export function testi64(): int32 {
    let a:int64 = 100;
    a--;
    return a as int32;
}

export function testf32(): float32 {
    let a:float32 = 1.16589f;
    a--;
    return a;
}

export function testf64(): float64 {
    let a:float64 = 1.68651;
    a--;
    return a;
}

export function testReturn(v:int32):int32{
    return v--;
}

export function testAssignToVariableDeclaration(_v:int32):int32{
    let a = _v--;
    return a;
}

export function testAssign(_v:int32):int32{
    let a = 0;
    a = _v--;
    return a;
}
